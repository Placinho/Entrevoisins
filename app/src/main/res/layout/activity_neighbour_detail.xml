<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".ui.neighbour_list.NeighbourDetail">

    <View
        android:id="@+id/view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#DFDDDD"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/nPicture"
        android:layout_width="422dp"
        android:layout_height="251dp"
        android:background="@android:color/white"
        android:contentDescription="New neighbour"
        android:adjustViewBounds="true"
        android:scaleType="fitXY"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/nName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="150dp"
        android:layout_marginBottom="575dp"
        android:layout_weight="2"
        android:background="@android:color/transparent"
        android:hint="My neighbours"
        android:padding="30dp"
        android:textColor="#F8F4F4"
        android:textColorHighlight="#F4F4F4"
        android:textSize="34sp"
        app:layout_constraintBottom_toTopOf="@+id/activity_favorite_greeting1_txt"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.227"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view"
        app:layout_constraintVertical_bias="0.553"
        tools:ignore="InvalidId,MissingConstraints,TextContrastCheck,UnknownId" />

    <TextView
        android:id="@+id/activity_favorite_greeting1_txt"
        android:layout_width="383dp"
        android:layout_height="254dp"
        android:layout_marginBottom="52dp"
        android:background="@drawable/linearborder"
        android:backgroundTint="#FFFF"
        android:contentDescription="CoordonnÃ©es"
        android:gravity="center"
        app:layout_constraintBottom_toTopOf="@id/activity_favorite_greeting2_txt"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.468"
        app:layout_constraintStart_toStartOf="@+id/view"
        tools:ignore="MissingContraints,UnknownId" />

    <TextView
        android:id="@+id/activity_favorite_greeting2_txt"
        android:layout_width="378dp"
        android:layout_height="146dp"
        android:layout_marginBottom="23dp"
        android:background="@drawable/linearborder"
        android:backgroundTint="#FFFF"
        android:contentDescription="A propos de moi"
        android:textAlignment="textStart"
        android:gravity="center_vertical"
        android:textColor="@android:color/black"
        android:textSize="13sp"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="@+id/view"
        app:layout_constraintEnd_toEndOf="@+id/activity_favorite_greeting1_txt"
        tools:visibility="visible"
        tools:ignore="RtlCompat" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/fav_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="228dp"
        android:layout_marginEnd="130dp"
        android:layout_marginBottom="10dp"
        android:background="@android:color/transparent"
        android:backgroundTint="#FFFF"
        android:clickable="true"
        android:contentDescription="Ajout Favoris"
        android:drawable="@drawable/ic_star_white_24dp"
        android:focusable="true"
        android:onClick="@drawable/ic_star_white_24dp"
        android:src="@drawable/ic_star_border_white_24dp"
        app:backgroundTint="#FFFF"
        app:layout_constraintBottom_toTopOf="@+id/activity_favorite_greeting1_txt"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toEndOf="@+id/activity_favorite_greeting1_txt"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:maxImageSize="32dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/nName2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="200dp"
        android:layout_marginEnd="290dp"
        android:layout_marginBottom="256dp"
        android:text="My neighbours"
        android:textColor="#090909"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@id/activity_favorite_greeting2_txt"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.996"
        app:layout_constraintStart_toEndOf="@id/activity_favorite_greeting2_txt" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="38dp"
        android:layout_height="31dp"
        android:layout_marginStart="11dp"
        android:layout_marginTop="200dp"
        android:layout_marginEnd="740dp"
        android:layout_marginBottom="5dp"
        android:contentDescription="@android:string/ok"
        android:tint="#FF80AB"
        app:layout_constraintBottom_toTopOf="@+id/view"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.483"
        app:layout_constraintStart_toEndOf="@id/activity_favorite_greeting2_txt"
        app:layout_constraintTop_toBottomOf="@id/activity_favorite_greeting1_txt"
        app:layout_constraintVertical_bias="0.479"
        app:srcCompat="@drawable/ic_baseline_location_on_24" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="38dp"
        android:layout_height="31dp"
        android:layout_marginStart="13dp"
        android:layout_marginTop="525dp"
        android:layout_marginEnd="745dp"
        android:layout_marginBottom="2dp"
        android:contentDescription="@android:string/emptyPhoneNumber"
        android:tint="#FF80AB"
        app:layout_constraintBottom_toTopOf="@id/activity_favorite_greeting1_txt"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.482"
        app:layout_constraintStart_toEndOf="@id/activity_favorite_greeting1_txt"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.371"
        app:srcCompat="@drawable/ic_baseline_local_phone_24" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="38dp"
        android:layout_height="31dp"
        android:layout_marginStart="420dp"
        android:layout_marginTop="650dp"
        android:layout_marginEnd="750dp"
        android:layout_marginBottom="11dp"
        android:contentDescription="@android:string/copyUrl"
        android:src="@drawable/ic_baseline_language_24"
        android:tint="#FF80AB"
        app:layout_constraintBottom_toTopOf="@id/activity_favorite_greeting1_txt"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.484"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.411" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="169dp"
        android:layout_height="35dp"
        android:layout_marginStart="200dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="12dp"
        android:layout_marginBottom="250dp"
        android:text="A propos de moi"
        android:textColor="#0B0B0B"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/view"
        app:layout_constraintHorizontal_bias="0.446"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/activity_favorite_greeting2_txt"
        app:layout_constraintVertical_bias="0.548" />

    <TextView
        android:id="@+id/nPhoneNumber"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="300dp"
        android:layout_marginTop="625dp"
        android:layout_marginEnd="69dp"
        android:layout_marginBottom="91dp"
        android:contentDescription="PhoneNumber"
        android:text="PhoneNumber"
        app:layout_constraintBottom_toTopOf="@id/activity_favorite_greeting1_txt"
        app:layout_constraintEnd_toStartOf="@id/nPicture"
        app:layout_constraintHorizontal_bias="0.413"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.42" />

    <TextView
        android:id="@+id/nAdress"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="300dp"
        android:layout_marginTop="625dp"
        android:layout_marginEnd="69dp"
        android:layout_marginBottom="91dp"
        android:contentDescription="Adress"
        android:text="Adress"
        app:layout_constraintBottom_toTopOf="@id/activity_favorite_greeting1_txt"
        app:layout_constraintEnd_toStartOf="@id/nPicture"
        app:layout_constraintHorizontal_bias="0.375"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.544" />

    <TextView
        android:id="@+id/nSocialNetworks"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="300dp"
        android:layout_marginTop="625dp"
        android:layout_marginEnd="69dp"
        android:layout_marginBottom="91dp"
        android:contentDescription="SocialNetworks"
        android:text="SocialNetworks"
        app:layout_constraintBottom_toTopOf="@id/activity_favorite_greeting1_txt"
        app:layout_constraintEnd_toStartOf="@id/nPicture"
        app:layout_constraintHorizontal_bias="0.375"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.297" />


</android.support.constraint.ConstraintLayout>